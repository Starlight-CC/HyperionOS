-- <@COMPILE> --
-- Method="minify"
-- <@COMPILE_END> --
local framework={
    hex=require("hyperEXE"),
    bf=require("brainfuck")
}

return function(file)
    for i,v in pairs(framework) do
        local tag="--<@EXE="..i..">--"
        if string.sub(file,1,#tag)==tag then
            local _,func = pcall(v,file)
            if func then return func end
        end
    end
    local func=load(file)
    if func then return func end
    error("no framework accociated with file")
end